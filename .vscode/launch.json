{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run gveIoTControl-Map",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/gveiotcontrol-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/gveiotMap/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/Map",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/gveIoTControl/Map\"}"
      },
      "envFile": "${workspaceFolder}/Map/.env1",
      "preLaunchTask": "Build gveIoTControl"
    }
  ]
}